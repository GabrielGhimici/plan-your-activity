{
  "extends": "../tsconfig.json",
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "",
    "outDir": "../dist/server",
    "sourceMap": true,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2016",
    "types": ["reflect-metadata"],
    "lib": [
      "es2016"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
